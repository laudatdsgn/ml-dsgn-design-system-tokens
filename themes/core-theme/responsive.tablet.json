{
  "sizing": {
    "device": {
      "min-width": {
        "$type": "sizing",
        "$value": "1024"
      },
      "min-height": {
        "$type": "sizing",
        "$value": "1366"
      }
    },
    "control": {
      "lg": {
        "$type": "sizing",
        "$value": "{primitives.sizing.sz-48}"
      },
      "md": {
        "$type": "sizing",
        "$value": "{primitives.sizing.sz-40}"
      },
      "sm": {
        "$type": "sizing",
        "$value": "{primitives.sizing.sz-32}"
      }
    }
  },
