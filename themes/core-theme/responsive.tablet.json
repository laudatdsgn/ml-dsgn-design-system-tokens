      "rem-base": {
        "$type": "number",
        "$value": "16"
      }
    },
    "font-size": {
      "fluid": {
        "4xl": {
          "$type": "fontSizes",
          "$value": "roundTo(( {typeset.scale.rem-base} * pow( {typeset.scale.operators.multiplier}, 6)) / {typeset.scale.operators.factor}, 0) * {typeset.scale.operators.factor}"
        },
        "3xl": {
          "$type": "fontSizes",
          "$value": "roundTo(( {typeset.scale.rem-base} * pow( {typeset.scale.operators.multiplier}, 5)) / {typeset.scale.operators.factor}, 0) * {typeset.scale.operators.factor}"
        },
        "2xl": {
          "$type": "fontSizes",
          "$value": "roundTo(( {typeset.scale.rem-base} * pow( {typeset.scale.operators.multiplier}, 4)) / {typeset.scale.operators.factor}, 0) * {typeset.scale.operators.factor}"
        },
        "xl": {
